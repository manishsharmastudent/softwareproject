<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="model.Korting" table="KORTINGEN">
      <meta attribute="class-description">
         This class contains the Kortingen detail.
      </meta>
      <id name="kortingId" type="int" column="kortingId">
         <generator class="native"/>
      </id>
       <property name="active" column="active" type="boolean"/>
       <property name="beginDatum" column="beginDatum" type="date"/>
       <property name="omschrijving" column="omschrijving" type="string"/>
       <property name="procent" column="procent" type="double"/>
   </class>
</hibernate-mapping>