<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.StationCsv" table="STATIONSCSV">
        <meta attribute="class-description">
            This class contains the Abonnement detail.
        </meta>
        <id name="naam" type="string" column="name">
        </id>
        <property name="log" column="longitude" type="string"/>
        <property name="alt" column="latitude" type="string"/>
    </class>
</hibernate-mapping>