<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="model.TypeKaart" table="TYPES">
      <meta attribute="class-description">
         This class contains the Login detail.
      </meta>
      <id name="id" type="string" column="typeId">
         <generator class="native"/>
      </id>
      <property name="korting" column="kortingId" type="string"/>
      <property name="naam" column="naam" type="string"/>
      <property name="omschrijving" column="omschrijving" type="string"/>
   </class>
</hibernate-mapping>