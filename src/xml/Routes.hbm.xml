<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="model.Route" table="ROUTES">
      <meta attribute="class-description">
         This class contains the Abonnement detail.
      </meta>
      <id name="routeId" type="int" column="routeId">
         <generator class="native"/>
      </id>
       <property name="bestemmingId" column="bestemmingId" type="int"/>
       <property name="vertrekId" column="vertrekId" type="int"/>
   </class>
</hibernate-mapping>